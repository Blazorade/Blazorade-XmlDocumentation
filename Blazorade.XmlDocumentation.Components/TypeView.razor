@inherits TypeDocumentationBase

<Heading Level="1">@this.Documentation.Member.ToDisplayName() <TypeDeclaration Type="@this.Documentation.Member" /></Heading>

<SummaryNode Node="this.Documentation.Summary" />

<RemarksNode Node="@this.Documentation.Remarks" />

<ExampleNode Node="@this.Documentation.Example" />

<FieldList LibraryKey="@this.LibraryKey" TypeName="@this.TypeName">
    <HeadingTemplate>
        <Heading Id="fields" IsAnchor="true" Level="2">Fields</Heading>
    </HeadingTemplate>
</FieldList>

<PropertyList LibraryKey="@this.LibraryKey" TypeName="@this.TypeName">
    <HeadingTemplate>
        <Heading Id="properties" IsAnchor="true" Level="2">Properties</Heading>
    </HeadingTemplate>
</PropertyList>

<MethodList LibraryKey="@this.LibraryKey" TypeName="@this.TypeName" Filter="MethodList.ConstructorFilter">
    <HeadingTemplate>
        <Heading Id="constructors" IsAnchor="true" Level="2">Constructors</Heading>
    </HeadingTemplate>
</MethodList>

<MethodList LibraryKey="@this.LibraryKey" TypeName="@this.TypeName" Filter="MethodList.NonConstructorFilter">
    <HeadingTemplate>
        <Heading Id="methods" IsAnchor="true" Level="2">Methods</Heading>
    </HeadingTemplate>
</MethodList>

<EventList LibraryKey="@this.LibraryKey" TypeName="@this.TypeName">
    <HeadingTemplate>
        <Heading Id="events" IsAnchor="true" Level="2">Events</Heading>
    </HeadingTemplate>
</EventList>

@* Operators *@

@* See also *@